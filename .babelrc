{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components", 
      { 
        "ssr": true 
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@constants": "./src/constants",
          "@modules": "./src/modules",
          "@pages": "./src/pages"
        }
      }
    ]
  ]
}
